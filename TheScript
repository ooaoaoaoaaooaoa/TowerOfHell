local function DWCGLZ_fake_script() -- TextLabel_2.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_2)

	script.Parent.Text = "hey " .. game.Players.LocalPlayer.Name .. " This script is used for the game tower of hell. if it doesnt work cry about it"
end
coroutine.wrap(DWCGLZ_fake_script)()
