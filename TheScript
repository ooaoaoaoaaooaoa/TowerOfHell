-- Scripts:

local function BONCZ_fake_script() -- Gears.Script 
	local script = Instance.new('Script', Gears)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.ScrollingFrame.Visible = true
		script.Parent.Parent.a1.Visible = false
	end)
end
coroutine.wrap(BONCZ_fake_script)()
local function FUIFKZV_fake_script() -- Speed.LocalScript 
	local script = Instance.new('LocalScript', Speed)

	script.Parent.MouseButton1Click:Connect(function()
		game.ReplicatedStorage.Gear.speed:Clone().Parent = game.Players.LocalPlayer.Backpack
	end)
end
coroutine.wrap(FUIFKZV_fake_script)()
local function UUKSYV_fake_script() -- Jump.LocalScript 
	local script = Instance.new('LocalScript', Jump)

	script.Parent.MouseButton1Click:Connect(function()
		game.ReplicatedStorage.Gear.jump:Clone().Parent = game.Players.LocalPlayer.Backpack
	end)
end
coroutine.wrap(UUKSYV_fake_script)()
